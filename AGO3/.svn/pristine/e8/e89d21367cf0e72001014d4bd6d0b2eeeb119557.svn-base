package com.one.dto;

import java.util.Date;

public class DpHeadVO {
	private int odId; // 공통 사용
	// dp_head_od
	private String dpId;
	private String odMsg;
	private Date odDate;
	// dp_od_mem
	private String memEmail;
	// dp_od.opcl
	private int opcl;

	public int getOdId() {
		return odId;
	}

	public void setOdId(int odId) {
		this.odId = odId;
	}

	public String getDpId() {
		return dpId;
	}

	public void setDpId(String dpId) {
		this.dpId = dpId;
	}

	public String getOdMsg() {
		return odMsg;
	}

	public void setOdMsg(String odMsg) {
		this.odMsg = odMsg;
	}

	public Date getOdDate() {
		return odDate;
	}

	public void setOdDate(Date odDate) {
		this.odDate = odDate;
	}

	public String getMemEmail() {
		return memEmail;
	}

	public void setMemEmail(String memEmail) {
		this.memEmail = memEmail;
	}

	public int getOpcl() {
		return opcl;
	}

	public void setOpcl(int opcl) {
		this.opcl = opcl;
	}

	@Override
	public String toString() {
		return "DpHeadVO [odId=" + odId + ", dpId=" + dpId + ", odMsg=" + odMsg + ", odDate=" + odDate + ", memEmail=" + memEmail + ", opcl=" + opcl + "]";
	}

}

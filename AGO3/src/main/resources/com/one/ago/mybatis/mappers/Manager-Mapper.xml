<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Manager-Mapper">

	<select id="selectManager" resultType="manager">
		SELECT
		  MANAGER_ID
		, MANAGER_PWD
		FROM
		MANAGER_ACT
	</select>

</mapper>